  "license": "MIT",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore src -f stylish",
    "lint:json": "eslint --ignore-path .gitignore src -f json -o reports/lint.json",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test:unit": "jest --passWithNoTests",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "posttest:unit": "node jest-sonar",
    "posttest:e2e:ci": "node cypress-report"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ldiego73/ldiego73.github.io"
  },
  "bugs": {
    "url": "https://github.com/ldiego73/ldiego73.github.io/issues"
  },
  "jestSonar": {
    "reportPath": "reports/test",
    "reportFile": "result.xml",
    "indent": 4
  }
